<UserControl x:Class="WeiZhi3.Parts.FlowDocumentEmbedViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" x:Name="_this"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="_container">
        <TextBlock Text="{Binding ReasonPhrase,Mode=OneWay,ElementName=_this}" />
        <!--<FlowDocumentScrollViewer x:Name="_viewer"/>-->
    </Grid>
</UserControl>
